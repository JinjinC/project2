                                <tr>
                                    <th class="source-col-header">Traces</th>
                                    <th>开始时间</th>
                                    <th>耗时</th>
                                </tr>
                                </thead>
                                <tbody>
                                <tr>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                </tr>
                                </tbody>
                            </table>
                        </div>
                    </section>
                </div>

            </div>

        </div>

    </div>


    
       
       
   {{/replace}}

    {{#append "scripts"}}
        <!-- page libs -->
        <script src="lib/slimScroll/jquery.slimscroll.min.js"></script>
