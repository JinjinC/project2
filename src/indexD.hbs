        <script>
            $('th').click(function(e){
                var a = $(this).find('.sortedup');
                var b = $(this).find('.sorteddown');

                if (a.css('display') == 'none' && b.css('display') == 'none') {
                    a.show();
                }else{
                    if (a.css('display') != 'none') {
                        a.hide();
                        b.show();
                    }else{
                        if (a.css('display') == 'none') {
                            b.hide();
                        }
                    }
                }
                e.stopPropagation();
            })

            $('#cpuchart').mouseover(function(){
                $('#cputrend').show();
            })
            $('#cpuchart').mouseout(function(){
                $('#cputrend').hide();
            })
            $('#contentchart').mouseover(function(){
                $('#contenttrend').show();
            })
            $('#contentchart').mouseout(function(){
