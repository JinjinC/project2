{{#extend "layout"}}

    {{#replace "sidebar"}}
        {{> sidebar this conditionClass=conditionClass active="sql"}}
    {{/replace}}

    {{#replace "app_content"}}
    <div class="row">

        <div class="col-lg-5 col-md-5">
            <h2 class="page-title">SQL <small>SQL页面</small></h2>
        </div>

        <div class="col-lg-7 col-md-7">
            {{#block "picker"}}
                {{> picker }}
            {{/block}}
        </div>
    </div>

        <div class="row">
            <div class="col-lg-4">
                <div class="row">
                    <div class="col-md-12">
                       <section class="widget widget-tabs">
                        <header>
                            <ul class="nav nav-tabs">
                                <li class="active">
                                    <a href="#webTran" data-toggle="tab" style="font-size:14px; font-weight: 500">Database</a>
                                </li>
                               
                            </ul>
                        </header>
                        <div class="body tab-content">
                            <div id="webTran" class="tab-pane active clearfix">

                                <div class="body" style="background-color:transparent; padding:0;">
                                    <ul id="myTab" class="nav nav-tabs">
                                        <li class="active"><a href="#responseTime" data-toggle="tab" class="tabItem">响应时间</a></li>
                                        <li><a href="#throughput" data-toggle="tab" class="tabItem active">吞吐量</a></li>
                                    </ul>
                                    <div id="myTabContent" class="tab-content" >
                                        <div class="tab-pane fade in active" id="responseTime">
                                            <div class="dd" id="nestable1">
                                                <ol class="dd-list">
                                                    <li class="dd-item dd-collapsed" data-id="1">
                                                       <div class="dd-handle">
                                                            <i class="iconfont icon2-dd" style="float: right;">&#xe613;
                                                            </i>
                                                            <span style="float: right;">5.43 ms</span>
                                                            <div class="bar-graph" style="width:90%"></div>
                                                            <div class="bar-text"><span>users-insert</span></div>
                                                        </div>
                                                    </li>
                                                    <li class="dd-item dd-collapsed" data-id="2">
                                                       <div class="dd-handle">
                                                            <i class="iconfont icon2-dd" style="float: right;">&#xe613;
                                                            </i>
                                                            <span style="float: right;">0.43 ms</span>
                                                            <div class="bar-graph" style="width:10%"></div>
                                                            <div class="bar-text"><span>users-update</span></div>
                                                        </div>
                                                    </li>
                                                </ol>
                                            </div>
                                        </div>
                                       
                                        <div class="tab-pane fade" id="throughput">
                                            <div class="dd" id="nestable2">
                                                <ol class="dd-list">
                                                    <li class="dd-item dd-collapsed" data-id="1">
                                                        <div class="dd-handle">
                                                            <i class="iconfont icon2-dd" style="float: right;">&#xe613;
                                                            </i>
                                                            <span style="float: right;" style="width:100%">196,333 cpm</span>
                                                            <div class="bar-graph" style="width:95%"></div>
                                                            <div class="bar-text"><span>users-insert</span></div>
                                                        </div>
                                                    </li>
                                                    <li class="dd-item dd-collapsed" data-id="1">
                                                        <div class="dd-handle">
                                                            <i class="iconfont icon2-dd" style="float: right;">&#xe613;
                                                            </i>
                                                            <span style="float: right;" style="width:100%">333 cpm</span>
                                                            <div class="bar-graph" style="width:5%"></div>
                                                            <div class="bar-text"><span>users-update</span></div>
                                                        </div>
                                                    </li>
                                                </ol>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                           
                            </div>
                            
                        </div>
                    </section>
                    </div>

                </div>

            </div>

            <div class="col-lg-8">
                <div class="row">
                    <div class="col-md-12">
                         <section class="widget">
                            <header>
                                <h4>
                                    响应时间
                                </h4>
                                <div class="widget-controls">
                                    <a data-widgster="expand" title="Expand" href="#"><i class="glyphicon glyphicon-chevron-up"></i></a>
                                    <a data-widgster="collapse" title="Collapse" href="#"><i class="glyphicon glyphicon-chevron-down"></i></a>
                                </div>
                            </header>
                            <div class="body no-margin">
                                <div id="visits-chart1" class="chart visits-chart">
                                    <svg></svg>
                                </div>
                                
                            </div>
                        </section>

                    </div> 

                </div>
                <div class="row">
                    <div class="col-md-6">
                         <section class="widget">
                            <header>
                                <h4>
                                    吞吐量 - 操作类型
                                </h4>
                                <div class="widget-controls">
                                    <a data-widgster="expand" title="Expand" href="#"><i class="glyphicon glyphicon-chevron-up"></i></a>
                                    <a data-widgster="collapse" title="Collapse" href="#"><i class="glyphicon glyphicon-chevron-down"></i></a>
                                </div>
                            </header>
                            <div class="body no-margin">
                                <div id="visits-chart2" class="chart visits-chart">
                                    <svg></svg>
                                </div>
                               
                            </div>
                        </section>

                    </div>
                    <div class="col-md-6">
                         <section class="widget">
                            <header>
                                <h4>
                                    响应时间 - 操作类型
                                    
                                </h4>
                                <div class="widget-controls">
                                    <a data-widgster="expand" title="Expand" href="#"><i class="glyphicon glyphicon-chevron-up"></i></a>
                                    <a data-widgster="collapse" title="Collapse" href="#"><i class="glyphicon glyphicon-chevron-down"></i></a>
                                </div>
                            </header>
                            <div class="body no-margin">
                                <div id="visits-chart3" class="chart visits-chart">
                                    <svg></svg>
                                </div>
                               
                            </div>
                        </section>

                    </div>

                </div>
                <div class="row">
                    <div class="col-md-12">
                        <section class="widget">
                            <header>
                                <div class="widget-controls">
                                    <a data-widgster="expand" title="Expand" href="#"><i class="glyphicon glyphicon-plus"></i></a>
                                    <a data-widgster="collapse" title="Collapse" href="#"><i class="glyphicon glyphicon-minus"></i></a>
                                </div>
                            </header>
                            <div class="body">
                                <table class="table table-striped no-margin sources-table">
                                    <thead>
                                    <tr>
                                        <th class="source-col-header">最慢 SQL</th>
                                        <th>时间戳</th>
                                        <th>耗时</th>
                                    </tr>
                                    </thead>
                                    <tbody>
                                    <tr>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                    </tr>
                                    </tbody>
                                </table>
                            </div>
                        </section>
                    </div>

                </div>

            </div>

        </div>

       
    {{/replace}}

     {{#append "scripts"}}
        <!-- page libs -->
        <script src="lib/slimScroll/jquery.slimscroll.min.js"></script>
