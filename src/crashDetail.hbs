                //item点击，去除所有active状态，当前点击对象加active，隐藏对象显示，开始tanslate
                $('.dd-item').click(function  () {
                    $('.dd-handle').removeClass("active");
                    $(this).find('.dd-handle').addClass("active");
                    $('.float-card').show().addClass("animated fadeInLeftBig");
                    setTimeout(function (){
                        $('.float-card').removeClass("animated fadeInLeftBig");
                    },1000);
                    $('.float-card-title').text($(this).find('.bar-text').text());
                });
                $('.float-card .glyphicon-remove').click(function() {
                    $('.float-card').hide();
                    $('.dd-handle').removeClass("active");
                });

                $('.user-crash').click(function  () {
                    $('.float-card').show().addClass("animated fadeInRightBig");

                    setTimeout(function (){
                        $('.float-card').removeClass("animated fadeInRightBig");
                    },500);
                    $('#serve-value').text($(this).text());
                });

            });


            $('th').click(function(e){
                var a = $(this).find('.sortedup');
                var b = $(this).find('.sorteddown');

                if (a.css('display') == 'none' && b.css('display') == 'none') {
                    a.show();
                }else{
                    if (a.css('display') != 'none') {
                        a.hide();
                        b.show();
                    }else{
                        if (a.css('display') == 'none') {
                            b.hide();
                        }
                    }
                }
                e.stopPropagation();
            })

            $('#show-all').click(function(){
                $('#show-all').hide();
                $('#show-content').show();
            })
            $('#click-show').click(function(){
                $('#hide-section').toggleClass("show-section");
            })
           

        </script>

        
